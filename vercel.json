{
  "builds": [
    { "src": "src/pages/**/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/src/css/$1" },
    { "src": "/js/(.*)", "dest": "/src/js/$1" },
    { "src": "/components/(.*)", "dest": "/src/components/$1" },
    { "src": "/pages/(.*)", "dest": "/src/pages/$1" }
  ]
}
